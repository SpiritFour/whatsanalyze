{
  "name": "chat-analyzer-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "jest",
    "deploy": "nuxt generate && push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,vue}": "eslint --cache --fix",
    "*.{js,css,md,vue}": "prettier --write"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.15",
    "@mdi/js": "^5.9.55",
    "@nuxt/content": "^1.13.1",
    "chart.js": "^2.9.4",
    "core-js": "^3.8.3",
    "html2canvas": "^1.0.0-rc.7",
    "jszip": "^3.6.0",
    "npm": "^7.5.4",
    "nuxt": "^2.14.12",
    "push-dir": "^0.4.1",
    "redirect-ssl": "^2.0.0",
    "stopwords-de": "^0.3.0",
    "stopwords-en": "^0.3.0",
    "vue-chartjs": "^3.5.1",
    "vue-gtag": "^1.11.0",
    "whatsapp-chat-parser": "^3.1.0"
  },
  "devDependencies": {
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/vuetify": "^1.11.3",
    "@vue/test-utils": "^1.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-vue": "^7.6.0",
    "husky": "4.2.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "vue-jest": "^3.0.4"
  }
}
